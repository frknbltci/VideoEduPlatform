    <!-- ////////////////////////////////////////////////////////////////////////////-->
    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                <section class="flexbox-container">
                    <div class="col-12 d-flex align-items-center justify-content-center">
                        <div class="col-md-6 col-10 box-shadow-2 p-0">
                            <div class="card border-grey border-lighten-3 m-0">
                                <div class="card-header border-0 pb-0">
                                    <div class="card-title text-center">
                                        <img src="~/Assets/images/logom.png" alt="branding logo">
                                    </div>
                                    <h6 class="card-subtitle line-on-side text-muted text-center font-small-3 pt-2">
                                        <span>Hesap Oluşturun</span>
                                    </h6>
                                </div>
                                <div class="card-content">
                                    <div class="card-body">
                                        <form class="form-horizontal"  method="post"  onsubmit="return RegisterPost(this)" enctype="multipart/form-data">
                                            <div class="row">
                                                <div class="col-12 col-sm-6 col-md-6">
                                                    <fieldset class="form-group position-relative has-icon-left">
                                                        <input type="text" name="FirstName" id="FirstName" class="form-control input-lg"
                                                               placeholder="Adınız" tabindex="1" required data-validation-required-message="Lütfen Soyad Giriniz">
                                                        <div class="form-control-position">
                                                            <i class="ft-user"></i>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                                <div class="col-12 col-sm-6 col-md-6">
                                                    <fieldset class="form-group position-relative has-icon-left">
                                                        <input type="text" name="LastName" id="LastName" class="form-control input-lg"
                                                               placeholder="Soyadınız" tabindex="2" required data-validation-required-message="Lütfen Soyad Giriniz">
                                                        <div class="form-control-position">
                                                            <i class="ft-user"></i>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                            </div>
                                            <fieldset class="form-group position-relative has-icon-left">
                                                <input type="text" name="UserName" id="UserName" class="form-control input-lg"
                                                       placeholder="Kullanıcı Adınız" tabindex="3" required data-validation-required-message="Lütfen Kullanıcı Adı Giriniz">
                                                <div class="form-control-position">
                                                    <i class="ft-user"></i>
                                                </div>
                                                <div class="help-block font-small-3"></div>
                                            </fieldset>
                                            <fieldset class="form-group position-relative has-icon-left">
                                                <input type="email" name="Email" id="Email" class="form-control input-lg" placeholder="Mail Adresiniz"
                                                       tabindex="4" required data-validation-required-message="Lütfen Mail Adresi Giriniz">
                                                <div class="form-control-position">
                                                    <i class="ft-mail"></i>
                                                </div>
                                                <div class="help-block font-small-3"></div>
                                            </fieldset>
                                            <div class="row">
                                                <div class="col-12 col-sm-6 col-md-6">
                                                    <fieldset class="form-group position-relative has-icon-left">
                                                        <input type="password" name="Pass" id="Pass" class="form-control input-lg" data-validation-matches-match="ConfPass"
                                                               placeholder="Şifreniz" tabindex="5" required data-validation-required-message="Lütfen Şifrenizi Giriniz">
                                                        <div class="form-control-position">
                                                            <i class="la la-key"></i>
                                                        </div>
                                                        <div class="help-block font-small-3"></div>
                                                    </fieldset>
                                                </div>
                                                <div class="col-12 col-sm-6 col-md-6">
                                                    <fieldset class="form-group position-relative has-icon-left">
                                                        <input type="password" name="ConfPass" id="ConfPass" class="form-control input-lg"
                                                               placeholder="Şifre Tekrarı" tabindex="6" data-validation-matches-match="Pass"
                                                               required data-validation-matches-message="Lütfen Tekrar Şifrenizi Giriniz">
                                                        <div class="form-control-position">
                                                            <i class="la la-key"></i>
                                                        </div>
                                                        <div class="help-block font-small-3"></div>
                                                    </fieldset>
                                                </div>
                                            </div>
                                            <h6 class="card-subtitle line-on-side text-muted text-center font-small-3 pt-2">
                                                <span>İsteğe Bağlı Bilgiler-</span>

                                            </h6>
                                            <h6 class="card-subtitle line-on-side text-muted text-center font-small-3 pt-2">
                                                <span style="color:red">Önerilir</span>

                                            </h6>


                                            <div class="col-12 col-sm-12 col-md-12">
                                                <fieldset class="form-group position-relative has-icon-left">
                                                    <input type="text" name="Phone" id="Phone" class="form-control input-lg"
                                                           placeholder="Telefon" tabindex="5">
                                                    <div class="form-control-position">
                                                        <i class="la la-phone"></i>
                                                    </div>
                                                    <div class="help-block font-small-3"></div>
                                                </fieldset>
                                            </div>

                                            <div class="col-12 col-sm-12 col-md-12">
                                                <fieldset class="form-group position-relative has-icon-left">
                                                    <input type="date" name="BirhDate" id="BirhDate" class="form-control input-lg"
                                                           placeholder="Şifre Tekrarı" tabindex="6">
                                                    <div class="form-control-position">
                                                        <i class="la la-birthday-cake"></i>
                                                    </div>
                                                    <div class="help-block font-small-3"></div>
                                                </fieldset>
                                            </div>


                                            <div class="row">
                                                <div class="col-12 col-sm-6 col-md-6">
                                                    <button type="submit" class="btn btn-info btn-lg btn-block sendRegisterInfo"><i class="ft-user"></i> Kayıt Ol</button>
                                                </div>
                                                <div class="col-12 col-sm-6 col-md-6">
                                                    <a href="/Home/Index" class="btn btn-danger btn-lg btn-block"><i class="ft-unlock"></i> Giriş Yapmak İçin</a>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
